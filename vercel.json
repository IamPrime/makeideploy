{
  "name": "makeideploy",
  "version": 1,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/api/slack/(.*)",
      "destination": "/api/slack"
    },
    {
      "source": "/api/og/(.*)",
      "destination": "/api/og"
    },
    {
      "source": "/api/((?!og|slack).*)",
      "destination": "/api"
    }
  ]
}
